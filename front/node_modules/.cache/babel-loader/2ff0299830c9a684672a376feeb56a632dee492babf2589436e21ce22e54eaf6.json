ies() {\n            try {\n                const response = await axios.get('http://127.0.0.1:3000/category');\n                if (response.data.status === 'success') {\n                const categories = response.data.res.reduce((acc, category) => {\n                    acc[category.id] = category.name;\n                    return acc;\n                }, {});\n                this.categories = categories;\n                } else {\n                console.error('Failed to fetch categories:', response.data.message);\n                }\n            } catch (error) {\n                console.error('Error fetching categories:', error);\n            }\n        },\n    },\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAgBA,OAAOA,KAAI,MAAQ,OAAO;AAC1B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,gBAAgB,EAAE,GAAG;MACrBC,UAAU,EAAE,CAAC;IACjB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,eAAe,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAACC,GAAG,EAAE;MAChB,IAAI,CAACN,gBAAe,GAAIM,GAAG;IAC/B,CAAC;IACD,MAAMH,eAAeA,CAAA,EAAG;MACpB,IAAI;QACA,MAAMI,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,gCAAgC,CAAC;QAClE,IAAID,QAAQ,CAACR,IAAI,CAACU,MAAK,KAAM,SAAS,EAAE;UACxC,MAAMR,UAAS,GAAIM,QAAQ,CAACR,IAAI,CAACW,GAAG,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,QAAQ,KAAK;YAC3DD,GAAG,CAACC,QAAQ,CAACC,EAAE,IAAID,QAAQ,CAACE,IAAI;YAChC,OAAOH,GAAG;UACd,CAAC,EAAE,CAAC,CAAC,CAAC;UACN,IAAI,CAACX,UAAS,GAAIA,UAAU;QAC5B,OAAO;UACPe,OAAO,CAACC,KAAK,CAAC,6BAA6B,EAAEV,QAAQ,CAACR,IAAI,CAACmB,OAAO,CAAC;QACnE;MACJ,EAAE,OAAOD,KAAK,EAAE;QACZD,OAAO,CAACC,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACtD;IACJ;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}